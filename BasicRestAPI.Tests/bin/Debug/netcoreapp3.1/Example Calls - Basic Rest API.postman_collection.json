		{
			"name": "Insert test data - one garage",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "9fd03fc1-44ac-4190-932f-f8ab32f9e650",
						"exec": [
							"pm.test(\"status test\", function() {\r",
							"    pm.response.to.have.status(201);\r",
							"\r",
							"    const responseJson = pm.response.json();\r",
							"    pm.expect(responseJson.name).to.be.eql(\"Ford - Merksem\");\r",
							"    pm.expect(responseJson.id).to.be.a('number');\r",
							"\r",
							"    pm.environment.set(\"INSERTED_ID\", responseJson.id);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n  \"name\": \"{{INSERT_TEST_NAME}}\"\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
